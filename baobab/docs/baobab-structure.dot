digraph g {
  subgraph cluster_ui {  
    label = "";
    style="filled";
    color="lightgrey";
    
    node [shape=ellipse,style=filled,color=red,fillcolor=red,style=filled];vk;twitter;site;
    vk [label="VK"];
    twitter [label="Twitter"];
    site [rotate, label="Site"];
  }

   node [shape=box,fillcolor=palegreen,style="filled,rounded"]; exports; inter; datastore; model; ui;
   exports [label="Exports"];
   inter [label="Interpretator"];
   model [root,label="Data model"];
   ui [label="User Interface"];
   datastore [label="Datastore"];
   
   subgraph cluster_store {
    label = "";
    style="filled";
    color="lightgrey";
    
    node [shape=ellipse,style=filled,color=red,fillcolor=red,style=filled];mysql;nosql;
    mysql [label="MySQL"];
    nosql [label="noSQL"];
  }
  
  subgraph cluster_core {
    inter; exports;datastore
  }
  exports -> model [dir=both]
  inter -> model [dir=both]
  datastore -> model [dir=both]

  datastore -> mysql [dir=both]
  datastore -> nosql [dir=both]
  ui -> inter

  ui-> vk
  ui -> twitter
  ui -> site
}
